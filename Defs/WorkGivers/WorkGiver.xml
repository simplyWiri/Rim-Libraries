<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <WorkGiverDef>
        <defName>RL_ReadSkillBook</defName>
        <label>read skill book</label>
        <giverClass>Libraries.Jobs.WorkGiver_ReadSkillBook</giverClass>
        <workType>Research</workType>
        <priorityInType>20</priorityInType>
        <verb>read</verb>
        <gerund>reading</gerund>
        <scanThings>true</scanThings>
        <requiredCapacities>
            <li>Sight</li>
        </requiredCapacities>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>RL_HaulToShelf</defName>
        <label>haul books to shelves</label>
        <giverClass>Libraries.Jobs.WorkGiver_HaulBookToShelf</giverClass>
        <workType>Hauling</workType>
        <verb>return</verb>
        <gerund>hauling to shelf</gerund>
        <priorityInType>70</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>RL_CleanShelves</defName>
        <label>clean dust off shelves</label>
        <giverClass>Libraries.Jobs.WorkGiver_CleanShelf</giverClass>
        <workType>Cleaning</workType>
        <verb>dusting</verb>
        <gerund>sweeping dust</gerund>
        <priorityInType>10</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>

</Defs>